<%- include("./Partials/_admhead")  %>
    <div class="container mt-5">
        <form action="/admin/addDOC" method="post" enctype="multipart/form-data">
            <div class="form-group">
                <label for="titre">Le titre</label>
                <input type="text" name="titre" id="titre" class="form-control">
            </div>
            <div class="form-group">
                <label for="sommaire">Sommaire</label>
                <input type="text" name="sommaire" id="sommaire" class="form-control">
            </div>
            <div class="form-group">
                <label for="contenu">contenu</label>
                <textarea name="contenu" id="contenu" class="form-control"></textarea>
            </div>
            <div ass="form-group">
                <label for="date">La date</label>
                <input type="date" name="date" id="date" class="form-control">
            </div>
            <div class="form-group mb-4">
                <label for="image">Image</label>
                <input type="file" name="image" id="image" class="form-control">
            </div>
            <button type="submit" class="btn btn-success">Enregistrer</button>
        </form>
    </div>
